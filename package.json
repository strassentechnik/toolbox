{
  "name": "nadler-tools",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start":
      "webpack-dev-server --config ./dev.config.js --mode development --env.NODE_ENV=development",
    "build": "webpack --mode production --env.NODE_ENV=production",
    "deploy": "gh-pages-deploy -- 'Release'",
    "clean-source":
      "npx rimraf .circleci .babelrc .editorconfig .nvmrc .stylelintrc .eslintrc dev.config.js postcss.config.js README.md src webpack.config.js package.json yarn.lock"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "commit": "Release",
    "post": ["clean-source"],
    "noprompt": false
  },
  "babel": {
    "presets": ["env", "react", "stage-2"],
    "plugins": ["transform-object-rest-spread"]
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.3.9",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "gh-pages-deploy": "0.4.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "react-hot-loader": "^4.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.1",
    "stylelint-webpack-plugin": "^0.10.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "final-form": "^4.5.2",
    "inuitcss": "^6.0.0",
    "query-string": "^6.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-final-form": "^3.3.1",
    "react-rangeslider": "^2.2.0",
    "react-tippy": "^1.2.2",
    "sass-mq": "^4.0.2"
  }
}
